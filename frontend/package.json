    "@angular/platform-browser": "^21.1.4",
    "@angular/router": "^21.1.4",
    "@angular/service-worker": "^21.1.4",
    "@ngrx/effects": "^21.0.1",
    "@ngrx/operators": "^21.0.1",
    "@ngrx/signals": "^21.0.1",
    "@ngrx/store": "^21.0.1",
    "@ngrx/store-devtools": "^21.0.1",
    "echarts": "^6.0.0",
